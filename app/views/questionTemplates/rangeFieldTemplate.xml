<Alloy>
	<ItemTemplate id="rangeFieldTemplate" name="rangeFieldTemplate" class="basicTemplate">
        <View class="viewGroup">
			<View class="headerView" bindId="debugTitleView">
				<Label id="questionTitle" bindId="title" class="questionTitle" />
				<Button class="notesButton" onClick="onNotesClick" bindId="notesBackground"/>
			</View>
			
			<View class="questionView">
				<TextField id="valueTextFieldOne" class="inputField" bindId="displayValue" onBlur="onTextField1Blur" onFocus="onTextFieldFocus" />
				<TextField id="valueTextFieldTwo" class="inputField" bindId="displayValue2" onBlur="onTextField2Blur" onFocus="onTextFieldFocus" />
			</View>
		</View>
    </ItemTemplate>
</Alloy>
