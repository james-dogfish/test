<Alloy>
	<Tab title="questionsTab" active="false">
		<Window id="window" tabBarHidden="true" navBarHidden="true">
			<Toolbar platform="ios" top="20" borderTop="true" borderBottom="true" barColor="#084f6f">
				<Button class="systemButton" title="Back" onClick="backButtonClick" left="10" />
				<Label id="appTitle"/>
				<Button class="systemButton" id="menuButton" title="Menu" onSingletap="openMenu" right="10" />
			</Toolbar>
			<Require id="questionListView" src="questionRenderer" type="view"/>
		</Window>
	</Tab>
</Alloy>