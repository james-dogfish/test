<Alloy>
	<Window id="win" exitOnClose="true">
		<View id="background" onClick="closeWindowCallBack"/>
		<View id="gotoDisplayView">
			<Button id="firstUnanswered" class="gotoButton" onClick="firstUnansweredClick" title="First Unanswered"/>
			<Button id="resumeLastPosition" class="gotoButton" onClick="resumeLastPositionClick" title="Resume Last Position"/>
			<Button id="createCensus" class="gotoButton" onClick="createCensusClick" title="Create Census"/>
			<View class="ruler" />
			<View id="contaierView">
				 <Require id="masterView" src="gotoQuestionSectionWindow/goToQuestionMasterView" type="view"/>
				 <Require id="detailView" src="gotoQuestionSectionWindow/goToQuestionDetailView" type="view"/>  
			</View>
		</View>
		<Require id="selectCensusView" src="gotoQuestionSectionWindow/censusSelect/selectCensusView" type="view"/>
		<Require id="addCensusView" src="gotoQuestionSectionWindow/censusSelect/addCensusView" type="view"/>		
	</Window>
</Alloy>